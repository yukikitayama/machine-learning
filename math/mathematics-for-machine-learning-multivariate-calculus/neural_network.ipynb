   "source": [
    "W = np.array([\n",
    "    [-2, 4, -1], \n",
    "    [6, 0, -3]\n",
    "])\n",
    "b = np.array([0.1, -2.5])\n",
    "x = np.array([0.3, 0.4, 0.1])\n",
    "sigma = np.tanh\n",
    "\n",
    "a1 = sigma(np.matmul(W, x) + b)\n",
    "print(f'a1: {np.round(a1, 2)}')"
   ]
  }
